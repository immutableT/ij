<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Back">
        <Item>
          <IdeaActionID>Back</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Left' or 'Button4 Click'</ShortCut>
          <Description>Back</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Code Review">
        <Item>
          <IdeaActionID>ActivateCodeReviewToolWindow</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Shift+D'</ShortCut>
          <Description>Code Review</Description>
          <Count>8</Count>
        </Item>
      </Statistic>
      <Statistic Action="Copy">
        <Item>
          <IdeaActionID>$Copy</IdeaActionID>
          <ShortCut>'Ctrl+C' or 'Ctrl+Insert'</ShortCut>
          <Description>Copy</Description>
          <Count>10</Count>
        </Item>
      </Statistic>
      <Statistic Action="Exclude">
        <Item>
          <IdeaActionID>UsageView.Exclude</IdeaActionID>
          <ShortCut>'Delete'</ShortCut>
          <Description>Exclude</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="G4 Sync...">
        <Item>
          <IdeaActionID>G4Sync</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Shift+4'</ShortCut>
          <Description>G4 Sync...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Implementation(s)">
        <Item>
          <IdeaActionID>GotoImplementation</IdeaActionID>
          <ShortCut>'Ctrl+Alt+B' or 'Ctrl+Alt+Button1 Click'</ShortCut>
          <Description>Implementation(s)</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Maximize Tool Window">
        <Item>
          <IdeaActionID>MaximizeToolWindow</IdeaActionID>
          <ShortCut>'Ctrl+Shift+Quote'</ShortCut>
          <Description>Maximize Tool Window</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Paste">
        <Item>
          <IdeaActionID>$Paste</IdeaActionID>
          <ShortCut>'Ctrl+V' or 'Shift+Insert'</ShortCut>
          <Description>Paste</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project">
        <Item>
          <IdeaActionID>ActivateProjectToolWindow</IdeaActionID>
          <ShortCut>'Alt+1'</ShortCut>
          <Description>Project</Description>
          <Count>8</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rename...">
        <Item>
          <IdeaActionID>RenameElement</IdeaActionID>
          <ShortCut>'Shift+F6'</ShortCut>
          <Description>Rename...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rerun 'Blaze test TestDatabaseEncryptionOnCreateCluster (//cloud/kubernetes/server:validate_test)'">
        <Item>
          <IdeaActionID>Rerun</IdeaActionID>
          <ShortCut>'Ctrl+F5'</ShortCut>
          <Description>Rerun 'Blaze test TestDatabaseEncryptionOnCreateCluster (//cloud/kubernetes/server:validate_test)'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rerun 'Blaze test clients:kmsclient_test'">
        <Item>
          <IdeaActionID>Rerun</IdeaActionID>
          <ShortCut>'Ctrl+F5'</ShortCut>
          <Description>Rerun 'Blaze test clients:kmsclient_test'</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Revert...">
        <Item>
          <IdeaActionID>ChangesView.Revert</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Z'</ShortCut>
          <Description>Revert...</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Settings...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'Ctrl+Alt+S'</ShortCut>
          <Description>Settings...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Sync Project with BUILD Files">
        <Item>
          <IdeaActionID>Blaze.IncrementalSyncProject</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Shift+B'</ShortCut>
          <Description>Sync Project with BUILD Files</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <ShortCut>'Alt+F12'</ShortCut>
          <Description>Terminal</Description>
          <Count>18</Count>
        </Item>
      </Statistic>
      <Statistic Action="Upload Snapshot to Critique">
        <Item>
          <IdeaActionID>Piper.ChangeList.Snapshot</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Shift+U'</ShortCut>
          <Description>Upload Snapshot to Critique</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Version Control">
        <Item>
          <IdeaActionID>ActivateVersionControlToolWindow</IdeaActionID>
          <ShortCut>'Alt+9'</ShortCut>
          <Description>Version Control</Description>
          <Count>9</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="Event Log">
        <Item>
          <IdeaActionID>ActivateEventLogToolWindow</IdeaActionID>
          <Description>Event Log</Description>
        </Item>
      </Statistic>
    </option>
  </component>
</application>